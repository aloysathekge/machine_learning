    "import torch\n",
    "\n",
    "x = torch.tensor(3.0, requires_grad=True)\n",
    "\n",
    "# Our function f(x) = x^2\n",
    "y = x**2\n",
    "\n",
    "# Backprop (compute derivative)\n",
    "y.backward()\n",
    "\n",
    "print(\"Value:\", y.item())\n",
    "print(\"Derivative dy/dx:\", x.grad.item())\n"
   ]
  },
